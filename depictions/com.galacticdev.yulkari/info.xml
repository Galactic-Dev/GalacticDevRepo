<package>
	<id>com.galacticdev.yulkari</id>
	<name>Yulkari</name>
	<version>0.0.2</version>
	<compatibility>
		<firmware>
			<miniOS>13.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
		<package>mobilesubstrate</package>
	</dependencies>
	<shortDescription>Dismiss the in app safari view when opening web page in safari from app.</shortDescription>
  <descriptionlist>
    <description>This tweak has preferences where you can choose to open the web page in the Safari app or the in app browser.</description>
  </descriptionlist>
	<screenshots>
	<screenshot>
		<description>Preferences</description>
		<image>prefs.jpg</image>
	</screenshot>
	</screenshots>
	<changelog>
		<change>Fixed support for more apps</change>
	</changelog>
	<links>
		<link>
			<name>Reddit</name>
			<url>https://www.reddit.com/user/Galactic_Dev</url>
			<iconclass>fa fa-reddit</iconclass>
		</link>
    <link>
      <name>Github</name>
      <url>https://github.com/Galactic-Dev</url>
      <iconclass>fa fa-github</iconclass>
    </link>
    <link>
      <name>Twitter</name>
      <url>https://twitter.com/dev_galactic</url>
      <iconclass>fa fa-twitter</iconclass>
    </link>
	</links>
</package>
